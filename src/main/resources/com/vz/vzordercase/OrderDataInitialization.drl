package com.vz.vzordercase;

import org.jbpm.casemgmt.api.model.instance.CaseFileInstance;
import java.util.Map;
import java.util.Map.Entry;

rule "Process selection dataPrep" ruleflow-group "validate"
	dialect "mvel
    when
        $caseData : CaseFileInstance()
        $product : Product() from $caseData.getData("order").products
    then
     insert($product);
     System.out.println("prodcut inserted into memory" + $product.getName());
     end

